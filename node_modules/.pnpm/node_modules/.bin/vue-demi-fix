#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/kf/my-docs/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.12/node_modules/vue-demi/bin/node_modules:/mnt/e/kf/my-docs/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.12/node_modules/vue-demi/node_modules:/mnt/e/kf/my-docs/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.12/node_modules:/mnt/e/kf/my-docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/kf/my-docs/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.12/node_modules/vue-demi/bin/node_modules:/mnt/e/kf/my-docs/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.12/node_modules/vue-demi/node_modules:/mnt/e/kf/my-docs/node_modules/.pnpm/vue-demi@0.14.10_vue@3.5.12/node_modules:/mnt/e/kf/my-docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vue-demi/bin/vue-demi-fix.js" "$@"
else
  exec node  "$basedir/../vue-demi/bin/vue-demi-fix.js" "$@"
fi
